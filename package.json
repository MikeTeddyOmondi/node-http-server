{
  "name": "esbuild-node",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "pnpm run \"/dev:/\"",
    "start": "node src/server.js",
    "dev:tsc": "tsc --watch --preserveWatchOutput -moduleResolution nodenext",
    "dev:node": "node --watch dist/src/server.js",
    "db:migrations": "drizzle-kit generate --schema dist/src/db/schema.js --dialect postgresql --out ./migrations",
    "db:migrate": "drizzle-kit migrate --config drizzle.config.ts",
    "studio": "drizzle-kit studio --config drizzle.config.ts"
  },
  "keywords": [],
  "author": "MikeTeddyOmondi",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.10.5",
    "@types/pg": "^8.11.10",
    "@types/uuid": "^10.0.0",
    "drizzle-kit": "^0.30.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.38.3",
    "express": "^4.19.1",
    "pg": "^8.13.1",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "uuid": "^11.0.5"
  }
}
